#!/usr/bin/env node

var fs = require('fs')
  , vzs = require('../lib')
  ;

if (!process.argv[2] || process.argv[2] == '-h' || process.argv[2] == '--help') {
  console.log('Usage: vzs-js FILE');
  console.log('');

  process.exit(1);
};

fs.readFile(process.argv[2], {encoding: 'utf8'}, function(err, src) {
  if (err) throw err;

  console.log(vzs.compile(src, 'javascript'));
});

